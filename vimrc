syntax on
filetype indent off
filetype plugin off
set autoindent
set nolisp
set tabstop=4
set softtabstop=0
set noexpandtab
set shiftwidth=4
set background=dark
map h :wincmd h<CR>
map j :wincmd j<CR>
map k :wincmd k<CR>
map l :wincmd l<CR>
map H :tabp<CR>
map J :tabe 
map K :wall<CR>:! tmux-a<CR><CR>
map L :tabn<CR>

